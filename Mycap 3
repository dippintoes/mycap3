#include<iostream>
#include<string.h>
using namespace std;

int main()
{
    string name,city;
    int age;
    cout<<"Enter the name of the candidate :\n";
    getline(cin,name);
    cout<<"Enter the city of the candidate :\n";
    cin>>city;
    cout<<"Enter the present age of the candidate :\n";
    cin>>age;
    if(age>=18)
    {
        cout<<name<<" is eligible to vote!!!!";
    }
    else
    {
        cout<<"Sorry! " <<name<<" is not yet eligible to vote!!\n";
        cout<<"Minimum age to vote is 18 years";
    }
    return 0;
}
